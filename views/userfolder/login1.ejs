<!-- ====================== Start Signup Form ============= -->

<section class="padd-top-80 padd-bot-80">
  <div class="container">
      <div class="log-box">
        <form id="myform" action="loginprocess" method="post" >
          
             
          <input type="hidden" class="form-control" id="id" name="id" required placeholder="id" >
           
            <div class="col-md-6">
              <div class="form-group">
                <label>Email</label>
                <input type="text" class="form-control" id="username" name="email" required placeholder="Email" >
              </div>
            </div>
            
            <div class="col-md-4">
              <div class="form-group">
                <label>Password</label>
                <input type="password" class="form-control" id="password" name="password"  placeholder="********" required>
              </div>
            </div>
            
           
            <div class="col-md-12">
              <p><a href="forgotpassword">Forgot My Password?</a></p>
              <p><a href="register">New Sign Up?</a></p>
              <div class="form-group text-center mrg-top-15">
              
                <button type="submit" class="btn theme-btn btn-m full-width">LogIn</button>
              </div>
            </div>
			<div class="clearfix"></div>			
        </form>
      </div>
  </div>
</section>
<!-- ====================== End Signup Form ============= --> 

<section class="newsletter theme-bg" style="background-image:url(/user/assets/img/bg-new.png)">
  <div class="container">
    <div class="row">
